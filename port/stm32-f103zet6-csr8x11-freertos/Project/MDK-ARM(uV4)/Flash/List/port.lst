Nvoid vPortEnterCritical( void )
N{
N	portDISABLE_INTERRUPTS();
X	ulPortSetInterruptMask();
N	uxCriticalNesting++;
N	__dsb( portSY_FULL_READ_WRITE );
X	__dsb( ( 15 ) );
N	__isb( portSY_FULL_READ_WRITE );
X	__isb( ( 15 ) );
N
N	/* This is not the interrupt safe version of the enter critical function so
N	assert() if it is being called from an interrupt context.  Only API
N	functions that end in "FromISR" can be used in an interrupt.  Only assert if
N	the critical nesting count is 1 to protect against recursive calls if the
N	assert function also uses a critical section. */
N	if( uxCriticalNesting == 1 )
N	{
N		configASSERT( ( portNVIC_INT_CTRL_REG & portVECTACTIVE_MASK ) == 0 );
X		if( ( ( ( * ( ( volatile uint32_t * ) 0xe000ed04 ) ) & ( 0xFFUL ) ) == 0 ) == 0 ) { ulPortSetInterruptMask(); for( ;; ); };
N	}
N}
N/*-----------------------------------------------------------*/
N
Nvoid vPortExitCritical( void )
N{
N	configASSERT( uxCriticalNesting );
X	if( ( uxCriticalNesting ) == 0 ) { ulPortSetInterruptMask(); for( ;; ); };
N	uxCriticalNesting--;
N	if( uxCriticalNesting == 0 )
N	{
N		portENABLE_INTERRUPTS();
X		vPortClearInterruptMask( 0 );
N	}
N}
