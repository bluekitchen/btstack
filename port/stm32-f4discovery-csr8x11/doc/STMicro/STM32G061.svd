            <name>AES_KEYR2</name>
            <displayName>AES_KEYR2</displayName>
			<description>AES key register 2</description>
            <addressOffset>0x18</addressOffset>
            <size>0x20</size>
            <resetValue>0x00000000</resetValue>
            <resetMask>0xFFFFFFFF</resetMask>
            <fields>
              <field>
