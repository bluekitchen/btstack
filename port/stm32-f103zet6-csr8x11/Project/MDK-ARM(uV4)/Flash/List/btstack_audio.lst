Ntypedef struct {
N
N    /**
N     * @brief Setup audio codec for specified samplerate and number of channels
N     * @param Channels (1=mono, 2=stereo)
N     * @param Sample rate
N     * @param Playback callback
N     * @return 1 on success
N     */
N    int (*init)(uint8_t channels,
N                uint32_t samplerate, 
N                void (*playback) (int16_t * buffer, uint16_t num_samples));
N
N    /**
N     * @brief Set volume
N     * @param Volume 0..127
N     */
N    void (*set_volume)(uint8_t volume);
N
N    /**
N     * @brief Start stream
N     */
N    void (*start_stream)(void);
N
N    /** 
N     * @brief Stop stream
N     */
N    void (*stop_stream)(void);
N
N    /**
N     * @brief Close audio codec
N     */
N    void (*close)(void);
N
N} btstack_audio_sink_t;
N
N
