N/*
N*********************************************************************************************************
N*	函 数 名: bsp_DetectKey
N*	功能说明: 检测一个按键。非阻塞状态，必须被周期性的调用。
N*	形    参:  按键结构变量指针
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nstatic void bsp_DetectKey(uint8_t i)
N{
N	KEY_T *pBtn;
N
N	/*
N		如果没有初始化按键函数，则报错
N		if (s_tBtn[i].IsKeyDownFunc == 0)
N		{
N			printf("Fault : DetectButton(), s_tBtn[i].IsKeyDownFunc undefine");
N		}
N	*/
N
N	pBtn = &s_tBtn[i];
N	if (pBtn->IsKeyDownFunc())
N	{
N		if (pBtn->Count < KEY_FILTER_TIME)
X		if (pBtn->Count < 5)
N		{
N			pBtn->Count = KEY_FILTER_TIME;
X			pBtn->Count = 5;
N		}
N		else if(pBtn->Count < 2 * KEY_FILTER_TIME)
X		else if(pBtn->Count < 2 * 5)
N		{
N			pBtn->Count++;
N		}
N		else
N		{
N			if (pBtn->State == 0)
N			{
N				pBtn->State = 1;
N
N				/* 发送按钮按下的消息 */
N				bsp_PutKey((uint8_t)(3 * i + 1));
N			}
N
N			if (pBtn->LongTime > 0)
N			{
N				if (pBtn->LongCount < pBtn->LongTime)
N				{
N					/* 发送按钮持续按下的消息 */
N					if (++pBtn->LongCount == pBtn->LongTime)
N					{
N						/* 键值放入按键FIFO */
N						bsp_PutKey((uint8_t)(3 * i + 3));
N					}
N				}
N				else
N				{
N					if (pBtn->RepeatSpeed > 0)
N					{
N						if (++pBtn->RepeatCount >= pBtn->RepeatSpeed)
N						{
N							pBtn->RepeatCount = 0;
N							/* 常按键后，每隔10ms发送1个按键 */
N							bsp_PutKey((uint8_t)(3 * i + 1));
N						}
N					}
N				}
N			}
N		}
N	}
N	else
N	{
N		if(pBtn->Count > KEY_FILTER_TIME)
X		if(pBtn->Count > 5)
N		{
N			pBtn->Count = KEY_FILTER_TIME;
X			pBtn->Count = 5;
N		}
N		else if(pBtn->Count != 0)
N		{
N			pBtn->Count--;
N		}
N		else
N		{
N			if (pBtn->State == 1)
N			{
N				pBtn->State = 0;
N
N				/* 发送按钮弹起的消息 */
N				bsp_PutKey((uint8_t)(3 * i + 2));
N			}
N		}
N
N		pBtn->LongCount = 0;
N		pBtn->RepeatCount = 0;
N	}
N}
